<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="5dp"
    android:padding="5dp"
    android:orientation="horizontal">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="5dp"

        android:id="@+id/view" >

        <RelativeLayout android:layout_height="170dp"
            android:padding="5dp"
            android:elevation="5dp"
            android:id="@+id/vcardId"
            android:layout_width="match_parent"
            >
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="@drawable/inicon"
               android:layout_marginLeft="5dp"
                android:layout_marginTop="12dp"
                />
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="90dp"

                android:layout_height="90dp"

                android:id="@+id/visitorImage"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />


            <RelativeLayout android:layout_height="190dp"

                android:orientation="vertical"

                android:layout_width="match_parent"
                android:id="@+id/relativeLayout"
                android:layout_toEndOf="@+id/visitorImage"
                android:layout_toRightOf="@+id/visitorImage"
                android:layout_alignParentBottom="true">

                <RelativeLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginTop="24dp"
                    android:id="@+id/namelinearLayout"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Name : "

                        android:id="@+id/textView4"


                        />
                    <TextView
                        android:id="@+id/visitorNameId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="AJAY M"
                        android:textStyle="italic"
                        android:textSize="15dp"

                        android:layout_gravity="center_horizontal"

                        android:layout_toRightOf="@+id/textView4"
                        android:layout_toEndOf="@+id/textView4"
                        android:layout_marginLeft="10dp"
                        />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_below="@+id/mobileLinerarlayout"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="80dp"
                    android:id="@+id/linearLayout">

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/mobileLinerarlayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/namelinearLayout"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mobile : "


                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/textView6" />
                    <TextView
                        android:id="@+id/visitorMobileId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="italic"
                        android:text=""
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="55dp"
                        />

                    <Button

                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:gravity="center"


                        android:textColor="@color/white"
                        android:background="@drawable/phoneicon"
                        android:id="@+id/visitorcallid"
                        android:layout_alignTop="@+id/textView6"
                        android:layout_alignBottom="@+id/visitorMobileId"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/outtimeLinerarlayout"
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_below="@+id/linearLayout"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">


                    <LinearLayout
                        android:id="@+id/flatLinerarlayout"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/mobileLinerarlayout"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Work : "

                            />
                        <TextView
                            android:id="@+id/visitorFlatId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="208"
                            android:textSize="15dp"
                            android:textStyle="italic"
                            android:layout_marginLeft="5dp"
                            />

                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </LinearLayout>

                <RatingBar
                    android:theme="@style/RatingBar"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:numStars="5"
                    android:clickable="false"
                    android:rating="3.5"
                    android:stepSize="0.1"
                    android:scaleX="0.5"
                    android:scaleY="0.5"
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    android:isIndicator="true"
                    android:layout_below="@+id/outtimeLinerarlayout"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />


            </RelativeLayout>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:background="@color/white"

                android:layout_alignLeft="@+id/visitorImage"
                android:layout_alignStart="@+id/visitorImage">






                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dp"
                    android:layout_below="@+id/verifyIconListId"
                    android:background="#c0c0c0"/>

            </RelativeLayout>

        </RelativeLayout>

    </android.support.v7.widget.CardView>
</RelativeLayout>

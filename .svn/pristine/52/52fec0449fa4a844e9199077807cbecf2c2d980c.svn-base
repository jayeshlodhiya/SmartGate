<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="5dp"
    android:padding="5dp"
    android:orientation="horizontal">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="5dp"

        android:id="@+id/view">

        <RelativeLayout
            android:layout_height="240dp"
            android:padding="5dp"
            android:elevation="5dp"
            android:layout_width="match_parent">

            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="90dp"

                android:layout_height="90dp"

                android:id="@+id/visitorImage"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="3dp"
                android:layout_marginTop="5dp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />


            <RelativeLayout
                android:layout_height="190dp"

                android:orientation="vertical"

                android:layout_width="match_parent"
                android:id="@+id/relativeLayout"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/visitorImage"
                android:layout_toEndOf="@+id/visitorImage">

                <RelativeLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginTop="57dp"
                    android:id="@+id/namelinearLayout"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Name : "

                        android:id="@+id/textView4"


                        />

                    <TextView
                        android:id="@+id/visitorNameId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="AJAY M"
                        android:textStyle="italic"
                        android:textSize="15dp"

                        android:layout_gravity="center_horizontal"

                        android:layout_toRightOf="@+id/textView4"
                        android:layout_toEndOf="@+id/textView4"
                        android:layout_marginLeft="10dp" />


                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_below="@+id/mobileLinerarlayout"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="80dp">

                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/mobileLinerarlayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/namelinearLayout"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mobile : "


                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/textView6" />

                    <TextView
                        android:id="@+id/visitorMobileId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="italic"
                        android:text=""
                        android:textSize="15dp"
                        android:gravity="center"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="55dp" />

                    <Button

                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:gravity="center"


                        android:textColor="@color/white"
                        android:background="@drawable/phoneicon"
                        android:id="@+id/visitorcallid"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true" />

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/outtimeLinerarlayout"
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_below="@+id/mobileLinerarlayout"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true">


                    <LinearLayout
                        android:id="@+id/flatLinerarlayout"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/mobileLinerarlayout"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Visiting : "

                            />

                        <TextView
                            android:id="@+id/visitorFlatId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="208"
                            android:textSize="15dp"
                            android:textStyle="italic"
                            android:layout_marginLeft="5dp" />

                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:background="@color/white"

                android:layout_alignLeft="@+id/visitorImage"
                android:layout_alignStart="@+id/visitorImage">

                <ImageView
                    android:id="@+id/verifyIconListId"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:background="@drawable/verifiedicon"
                    android:layout_gravity="right"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="sdhfbh"
                    android:gravity="center"
                    android:textAlignment="center"

                    android:id="@+id/visitorNumnber"
                    android:textSize="15dp"
                    android:textColor="@color/light_black"
                    android:textStyle="bold"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />


                <View
                    android:layout_width="fill_parent"
                    android:layout_height="2dp"
                    android:layout_below="@+id/verifyIconListId"
                    android:background="#c0c0c0" />

            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="2dp"
                android:layout_below="@+id/relativeLayout"
                android:background="#c0c0c0"
                android:id="@+id/view2" />

            <RelativeLayout
                android:id="@+id/intimeLinerarlayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_below="@+id/view2"
                android:layout_alignLeft="@+id/view2"
                android:layout_alignStart="@+id/view2"
                android:layout_alignParentBottom="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="IN : "

                    android:id="@+id/textView5"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:id="@+id/vintimeId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10-03-2018"
                    android:textSize="13dp"
                    android:textStyle="italic"
                    android:layout_alignParentBottom="true"
                    android:layout_toRightOf="@+id/textView5"
                    android:layout_toEndOf="@+id/textView5" />

                <TextView
                    android:id="@+id/vouttimeId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="13dp"

                    android:textStyle="italic"
                    android:layout_alignParentBottom="true"
                    android:layout_toRightOf="@+id/textView3"
                    android:layout_toEndOf="@+id/textView3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OUT : "

                    android:id="@+id/textView3"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/vintimeId"
                    android:layout_toEndOf="@+id/vintimeId"
                    android:layout_marginLeft="30dp"
                    android:layout_marginStart="30dp" />
            </RelativeLayout>

        </RelativeLayout>

    </android.support.v7.widget.CardView>
</RelativeLayout>
